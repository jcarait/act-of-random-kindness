      <div class="card-taskid">
        <h4 style="text-decoration: underline;">{{tasks.task_name}}</h4>
        <h6 style="text-decoration: underline;">Task by</h6>
        <p>{{tasks.creator.user_name}}</p>
        <h6 style="text-decoration: underline;">Task Description</h6>
        <p>{{tasks.description}}</p>
        <p>Length of task : {{tasks.duration}} minutes</p>
        <p>Status : {{tasks.status}}</p>
        <p>Last updated : {{tasks.updatedAt}}</p>
      </div>
